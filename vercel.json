{
    
    "builds": [
      {
        "src": "gestion_services/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb", "runtime":"python3.12.7"
        }
      }
      
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "gestion_services/wsgi.py"
      }
    ]
  }
  